{
  "Provider": "vault",
  "Config": {
      "Address": "{{ vault_addr }}",
      "Token": "{{ vault_connect_token.stdout }}",
      "RootPKIPath": "pki",
      "IntermediatePKIPath": "pki_int",
      "LeafCertTTL": "72h",
      "RotationPeriod": "2160h",
      "IntermediateCertTTL": "8760h",
      "PrivateKeyType": "rsa",
      "PrivateKeyBits": 2048
  },
  "ForceWithoutCrossSigning": false
}
